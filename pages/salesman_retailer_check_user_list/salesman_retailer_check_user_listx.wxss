/* pages/salesman_retailer_check_user_list/salesman_retailer_check_user_listx.wxss */
.items {
  background: #fff;
}
.item {
  border-bottom: 1px solid #e5e5e5;
  min-height: 200rpx;
  padding: 30rpx 26rpx 37rpx;
  display: flex;
}
.item:last-child {
  border: none;
}
.item .img {
  width: 130rpx;
  height: 130rpx;
  background: #1aa93a;
  border-radius: 50%;
  margin-right: 40rpx;
  margin-top: 5rpx;
}
.item .mid {
  flex: 1;
  min-width: 0;
}
.item .mid .name {
  font-size: 38rpx;
  color: #333333;
  line-height: 1.1;
  margin-bottom: 25rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  white-space: nowrap;
}
.real-name {
  margin-right: 35rpx;
}
.mobile,
.address {
  font-size: 28rpx;
  color: #999999;
  line-height: 1.2;
  margin-bottom: 5rpx;
}
